{
  "family": "lks-gatus-task",
  "taskRoleArn": "arn:aws:iam::203328187809:role/LabRole",
  "executionRoleArn": "arn:aws:iam::203328187809:role/LabRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": "FARGATE",
  "cpu": "1024",
  "memory": "1024",


  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false,
  "containerDefinitions": [
    {
      "name": "lks-gatus",
      "image": "<task-def-image>",
      "cpu": 0,
      "essential": true,

      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks-gatus-task",
          "awslogs-region": "us-east-1",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
