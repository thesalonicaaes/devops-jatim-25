  "networkMode": "awsvpc",
  "requiresCompatibilities": "FARGATE",
  "cpu": "1024",
  "memory": "3072",
  "volumes": [
    {
      "efsVolumeConfiguration": {
        "authorizationConfig": {
          "accessPointId": "fsap-0d04dea2a261da936",
          "iam": "ENABLED"
        },
        "fileSystemId": "fs-0296e0e275e130cf3",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED"
      },
      "name": "vol-public_userfile"
    },
    {
      "efsVolumeConfiguration": {
        "authorizationConfig": {
          "accessPointId": "fsap-0585acf0631253601",
          "iam": "ENABLED"
        },
        "fileSystemId": "fs-0296e0e275e130cf3",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED"
      },
      "name": "vol-userfile"
    },
    {
      "efsVolumeConfiguration": {
        "authorizationConfig": {
          "accessPointId": "fsap-063a8eb701fdc5c33",
          "iam": "ENABLED"
        },
        "fileSystemId": "fs-0296e0e275e130cf3",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED"
      },
      "name": "vol-plugins"
    }
  ],


  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false,

  "containerDefinitions": [
    {
      "name": "leantime",
      "image": "leantime/leantime:latest",
      "cpu": 0,
      "essential": true,
      "volumesFrom": [],
      "systemControls": [],

      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "leantime-8080-tcp",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "LEAN_DB_DATABASE",
          "valueFrom": "/lks/db/name"
        },
        {
          "name": "LEAN_DB_HOST",
          "valueFrom": "/lks/db/host"
        },
        {
          "name": "LEAN_DB_PASSWORD",
          "valueFrom": "/lks/db/password"
        },
        {
          "name": "LEAN_DB_PORT",
          "valueFrom": "/lks/db/port"
        },
        {
          "name": "LEAN_DB_USER",
          "valueFrom": "/lks/db/username"
        }
      ],
    }
  ],
      "environment": [],
      "mountPoints": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks-leantime-task",
          "awslogs-region": "us-east-1",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-stream-prefix": "ecs"
        }
      }
}
